<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.prima.projectlaravelpresales.FormBuyerIdentificationSearching">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buyeridentificationsearchingEdittextSearching"
        android:hint="Masukkan NIK"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/buyeridentificationsearchingButtonSearching" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:id="@+id/buyeridentificationsearchingButtonSearching"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />
    <android.support.v7.widget.RecyclerView
        android:id="@+id/buyeridentificationsearchingrecyclerview"
        android:layout_width="match_parent"
        android:layout_height="200dp"

        android:layout_below="@+id/buyeridentificationsearchingEdittextSearching"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true" />


</RelativeLayout>
